<div class="bids-group no-padding">
  <div class="js-bids-operator bids-content content-border clearfix" data-users="{{json _DATA_}}">
    <div class="group-left">
      <div class="control-group">
        <label class="group-title"><b class="required">*</b>采购经办人</label>
        <select name="issueBidOperatorId">
          {{#each _DATA_}}
            <option value="{{userId}}" {{#equals userId ../userid}}selected{{/equals}}>{{realName}}</option>
          {{/each}}
        </select>
      </div>
    </div>
    <div class="group-left">
      <div class="control-group">
        <label class="group-title"><b class="required">*</b>答疑经办人</label>
        <select name="qaOperatorId">
          {{#each _DATA_}}
            <option value="{{userId}}" {{#equals userId ../userid}}selected{{/equals}}>{{realName}}</option>
          {{/each}}
        </select>
      </div>
    </div>
  </div>
</div>
