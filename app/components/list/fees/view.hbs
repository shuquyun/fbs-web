{{#component "component-fees js-comp"}}
<div class="component-body" >
    <div class="list-tree-nav body-left js-fee-category-list" data-category-list="{{json _DATA_}}">
    </div>
    <div class="body-right js-fee-item-table hide">
      {{> component:list/fee_sum/all_templates/wrap}}
      <table class="user-manage-table table table-horizontal center-text">
        <thead>
          <tr>
            <th width="20%">项目名称</th>
            <th width="25%">计价基础</th>
            <th width="10%">计价基数</th>
            <th width="10%">费率（%）</th>
            <th width="10%">费用</th>
            <th width="15%">备注</th>
            <th class="td-operation" width="10%">操作</th>
          </tr>
        </thead>
        <tbody class="js-fee-item-tbody">
          {{!-- {{#each _ITEMATTR_}}
            {{> component:valuation/template_fee_manage/all_templates/tmpl_item data=this}}
          {{/each}} --}}

        </tbody>
      </table>
      <button class="btn js-update-fee-item" style="margin-top: 10px;">添加新的清单行</button>

    </div>

  </div>
{{/component}}
