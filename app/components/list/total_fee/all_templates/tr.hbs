<tr data-id="{{data.id}}" class="js-tmpl-fee-item-tr" data-fee={{json data}}>
  <td>
    {{data.id}}
  </td>
  <td>
    {{data.name}}
  </td>
  <td>{{data.projectFeature}}</td>
  <td>{{data.valuationBase}}</td>
  <td>{{data.feeRate}}</td>
  <td>{{data.fee}}</td>
  <td>{{data.remark}}</td>
  <td class="td-operation">
    <a href="javascript:void(0)" class="js-update-fee-item" data-id="{{data.id}}">编辑</a>
  </td>
</tr>
